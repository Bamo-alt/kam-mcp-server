{
  "name": "kam-mcp-server",
  "version": "0.0.3",
  "keywords": [
    "mcp",
    "server",
    "kam"
  ],
  "license": "MIT",
  "author": "Bamo-alt",
  "type": "module",
  "description": "KAM MCP server",
  "bin": {
    "kam-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bamo-alt/kam-mcp-server.git"
  },
  "scripts": {
    "clear": "rm -rf dist/*",
    "build": "npm run clear && tsc && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
