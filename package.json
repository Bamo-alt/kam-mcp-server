    "start": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "fastmcp": "^1.22.4",
    "uuid": "^11.1.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.31",
    "@types/ws": "^8.18.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.18.0"
  }
}